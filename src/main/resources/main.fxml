        <Label layoutX="14.0" layoutY="113.0" text="Alphabet:" AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="113.0" />
        <ChoiceBox fx:id="choiceBox_alphabet" layoutX="83.0" layoutY="108.0" prefWidth="150.0" AnchorPane.leftAnchor="96.0" AnchorPane.topAnchor="108.0" />
        <Button fx:id="button_regex" layoutX="524.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#onClick_buttonRegex" text="Enter" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="108.0" />
        <Label layoutX="14.0" layoutY="142.0" text="Graph (Dot Notation):" AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="144.0" />
